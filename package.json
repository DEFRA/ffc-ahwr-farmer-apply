{
  "name": "ffc-ahwr-farmer-apply",
  "version": "0.39.16",
  "description": "Web frontend for the farmer apply journey",
  "homepage": "https://github.com/DEFRA/ffc-ahwr-farmer-apply",
  "main": "app/index.js",
  "scripts": {
    "build": "webpack",
    "build:watch": "webpack --watch",
    "pretest": "npm run test:lint",
    "test": "jest --runInBand",
    "test:watch": "jest --coverage=false --onlyChanged --watch --runInBand",
    "test:watch:all": "npm-run-all --parallel test:watch build:watch",
    "test:lint": "standard",
    "test:lint-fix": "standard --fix",
    "test:debug": "node --inspect-brk=0.0.0.0 ./node_modules/jest/bin/jest.js --coverage=false --onlyChanged --watch --runInBand --no-cache",
    "start:watch": "npm-run-all --parallel build:watch start:nodemon",
    "start:debug": "nodemon --inspect-brk=0.0.0.0 --ext css,js,njk --legacy-watch app/index.js",
    "start:nodemon": "nodemon --inspect=0.0.0.0 --ext css,js,njk --legacy-watch app/index.js"
  },
  "author": "Defra",
  "contributors": [
    "Steve Dickinson steve.dickinson@defra.gov.uk",
    "Abidemi Adio abidemi.adio@defra.gov.uk"
  ],
  "license": "OGL-UK-3.0",
  "dependencies": {
    "@azure/identity": "^3.2.2",
    "@hapi/boom": "^10.0.1",
    "@hapi/catbox-redis": "^7.0.1",
    "@hapi/cookie": "^12.0.1",
    "@hapi/crumb": "^9.0.1",
    "@hapi/hapi": "^21.3.2",
    "@hapi/inert": "^7.1.0",
    "@hapi/vision": "^7.0.1",
    "@hapi/wreck": "^18.0.1",
    "@hapi/yar": "^11.0.1",
    "applicationinsights": "^2.6.0",
    "crypto-js": "^4.1.1",
    "ffc-ahwr-event-publisher": "1.0.2",
    "ffc-messaging": "^2.7.0",
    "govuk-frontend": "4.6.0",
    "hapi-pino": "^12.1.0",
    "joi": "17.9.2",
    "json5": "^2.2.3",
    "jsonwebtoken": "^9.0.0",
    "jwk-to-pem": "^2.0.5",
    "notifications-node-client": "^7.0.0",
    "nunjucks": "^3.2.4",
    "tsconfig-paths": "^4.2.0",
    "uuid": "^9.0.0"
  },
  "overrides": {
    "notifications-node-client": {
      "jsonwebtoken": "9.0.0"
    },
    "@azure/identity": {
      "jsonwebtoken": "9.0.0"
    },
    "fast-xml-parser": "4.1.2"
  },
  "devDependencies": {
    "@hapi/catbox-memory": "^6.0.1",
    "@types/cheerio": "^0.22.31",
    "@types/hapi__hapi": "^20.0.13",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.2.4",
    "blipp": "^4.0.2",
    "cheerio": "^1.0.0-rc.12",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.4",
    "dotenv": "^16.0.3",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.1",
    "jest": "^29.5.0",
    "jest-junit": "^16.0.0",
    "jest-when": "^3.5.2",
    "mini-css-extract-plugin": "^2.7.6",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^10.0.0",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.62.1",
    "sass-loader": "^13.3.0",
    "standard": "^17.0.0",
    "webpack": "^5.84.1",
    "webpack-cli": "^5.1.1"
  },
  "standard": {
    "env": {
      "jest": true
    },
    "globals": [
      "$",
      "browser"
    ],
    "ignore": [
      "app/frontend/dist/**/*.js",
      "test/acceptance/**/*.js",
      "test/acceptance/**/*.mjs",
      "test/acceptance/**/*.cjs"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "**/test-output/**"
    ]
  }
}
