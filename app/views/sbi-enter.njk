{% extends './layouts/layout.njk' %}

{% block pageTitle %}{{serviceName}}{% endblock %}

{% block beforeContent %}
  {{ govukBackLink({
    text: "Back",
    href: "/apply/register-your-interest/crn-enter",
	  attributes: {id: "back"}
  }) }}
{% endblock %}

{% block content %}
<div class="govuk-grid-row">
  <div class="govuk-grid-column-two-thirds">
    <form method="POST">
      <input type="hidden" name="crumb" value="{{crumb}}"/>
      <h1 class="govuk-heading-l">Enter the single business identifier (SBI) number</h1>
    </form>
  </div>
</div>
{% endblock %}
